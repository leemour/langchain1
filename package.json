{
  "name": "langchain1",
  "version": "1.0.0",
  "description": "",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=24.5.0"
  },
  "scripts": {
    "dev": "nodemon --exec tsx src/index.ts",
    "debug": "tsx src/index.ts --debug",
    "dev:inspect": "nodemon --inspect --exec tsx src/index.ts",
    "build": "tsup src/index.ts --format esm,cjs --dts --clean",
    "start": "node dist/index.js",
    "setup:qdrant": "tsx scripts/setup-qdrant.ts",
    "ingest": "tsx scripts/ingest-docs.ts",
    "ingest:debug": "tsx --inspect-brk scripts/ingest-docs.ts",
    "inspect": "tsx scripts/inspect-qdrant.ts",
    "clear": "tsx scripts/clear-qdrant.ts",
    "test:loaders": "tsx scripts/test-loaders.ts",
    "test:graph": "tsx scripts/test-graph.ts",
    "test:graph:debug": "tsx --inspect-brk scripts/test-graph.ts",
    "test:graph:inspect": "tsx --inspect scripts/test-graph.ts",
    "bot": "tsx scripts/telegram-bot.ts",
    "bot:dev": "nodemon --exec tsx scripts/telegram-bot.ts",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "lint": "biome check src",
    "format": "biome format --write src",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.25.0",
  "dependencies": {
    "@langchain/anthropic": "^1.3.0",
    "@langchain/classic": "^1.0.6",
    "@langchain/community": "^1.1.0",
    "@langchain/core": "^1.1.5",
    "@langchain/langgraph": "^1.0.4",
    "@langchain/openai": "^1.2.0",
    "@langchain/qdrant": "^1.0.1",
    "@langchain/textsplitters": "^1.0.1",
    "@qdrant/js-client-rest": "^1.16.2",
    "dotenv": "^17.2.3",
    "langchain": "^1.2.0",
    "langsmith": "^0.3.87",
    "pino": "^10.1.0",
    "pino-pretty": "^13.1.3",
    "telegraf": "^4.16.3",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.8",
    "@types/node": "^25.0.1",
    "@vitest/coverage-v8": "^4.0.15",
    "@vitest/ui": "^4.0.15",
    "nodemon": "^3.1.11",
    "tsup": "^8.5.1",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.15"
  }
}
